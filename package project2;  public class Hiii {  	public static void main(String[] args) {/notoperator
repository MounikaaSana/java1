package project2;

public class Hiii {

	public static void main(String[] args) {
		// ~ ! operators
		int a=5;
		int b=9;
		boolean c=true;
		boolean d=false;
	       System.out.println(a);
	       System.out.println(b);
	       System.out.println(c);
	       System.out.println(d);
	       System.out.println("changes to:");
	       System.out.println(~a);
	       System.out.println(~b);
	       System.out.println(!c);
	       System.out.println(!d);
	}

}
output:-
5
9
true
false
